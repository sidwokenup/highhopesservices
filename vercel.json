{
  "version": 2,
  "builds": [
    { "src": "api/contact.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/contact",
      "dest": "/api/contact.js"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|svg|jpg|jpeg|gif|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}